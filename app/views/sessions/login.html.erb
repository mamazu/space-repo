<h1>Login</h1>
	<%= form_tag '/login' do %>
	<div class="mui-textfield">
	<%= label_tag :username%>
	<%= text_field_tag :username %>
	</div>
	<div class="mui-textfield">
	<%= label_tag :password%>
	<%= password_field_tag :password%>
	</div>
	<%= submit_tag("Login", :class => 'mui-btn mui-btn--primary mui-btnraised') %>
<%end%>